{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "warn",
    "prettier/prettier": "error"
  },
  "globals": {
    "APP_NAME": "readonly",
    "APP_VER": "readonly",
    "CACHE_NAME": "readonly"
  }
}
